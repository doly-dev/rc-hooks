{
  "name": "rc-hooks",
  "version": "3.0.20",
  "description": "React Hooks Library.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "npm run build:es && npm run build:cjs && npm run build:umd",
    "build:es": "rm -rf es && tsc -p tsconfig.build.json -m ES2015 -t ES5 --outDir es -d",
    "build:cjs": "rm -rf lib && tsc -p tsconfig.build.json -m commonjs -t ES5 --outDir lib -d",
    "build:umd": "rm -rf dist && rollup -c",
    "doc": "dumi build",
    "doc-version": "cross-env BUILD_DOC_VERSION=true dumi build",
    "prettier": "prettier --write **/*",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "commit": "cz",
    "test": "jest --verbose",
    "prepare": "husky && dumi setup",
    "prepublishOnly": "npm test && npm run build",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": "eslint",
    "**/*.{js,jsx,ts,tsx,json,css,scss,sass,less,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doly-dev/rc-hooks.git"
  },
  "keywords": [
    "react",
    "hook"
  ],
  "author": "caijf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doly-dev/hooks/issues"
  },
  "homepage": "https://github.com/doly-dev/hooks#readme",
  "dependencies": {
    "tslib": "^2.8.1",
    "ut2": "^1.17.0",
    "util-helpers": "^5.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/cz-commitlint": "^19.8.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.17.24",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "antd": "^5.24.4",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dumi": "^2.4.18",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "inquirer": "^9.3.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "mockjs": "^1.1.0",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.36.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": ">= 16.8.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
